            "sql": {
              "_": "SELECT * from (select\ndataareaid,\nrating_year,\nrating_month,\nROUND(\"rta customer complaints\",2)-lead(ROUND(\"rta customer complaints\",2)) OVER (PARTITION BY driverid ORDER BY dataareaid desc,trans_date desc) as \"rta customer complaints\",\nROUND(\"ftc customer complaints\",2)-lead(ROUND(\"ftc customer complaints\",2)) OVER (PARTITION BY driverid ORDER BY dataareaid desc,trans_date desc) as \"ftc customer complaints\",\nROUND(\"itc customer complaints\",2)-lead(ROUND(\"itc customer complaints\",2)) OVER (PARTITION BY driverid ORDER BY dataareaid desc,trans_date desc) as \"itc customer complaints\",\nROUND(\"red accidents count\",2)-lead(ROUND(\"red accidents count\",2)) OVER (PARTITION BY driverid ORDER BY dataareaid desc,trans_date desc) as \"red accidents count\",\nROUND(\"traffic fines\",2)-lead(ROUND(\"traffic fines\",2)) OVER (PARTITION BY driverid ORDER BY dataareaid desc,trans_date desc) as \"traffic fines\",\nROUND(\"commission score\",2)-lead(ROUND(\"commission score\",2)) OVER (PARTITION BY driverid ORDER BY dataareaid desc,trans_date desc) as \"commission score\",\nROUND(\"rta quality & internal fines\",2)-lead(ROUND(\"rta quality & internal fines\",2)) OVER (PARTITION BY driverid ORDER BY dataareaid desc,trans_date desc) as \"rta quality & internal fines\",\nROUND(\"ftc quality fines\",2)-lead(ROUND(\"ftc quality fines\",2)) OVER (PARTITION BY driverid ORDER BY dataareaid desc,trans_date desc) as \"ftc quality fines\",\nROUND(\"itc quality fines\",2)-lead(ROUND(\"itc quality fines\",2)) OVER (PARTITION BY driverid ORDER BY dataareaid desc,trans_date desc) as \"itc quality fines\",\nROUND(\"revenue km%\",2)-lead(ROUND(\"revenue km%\",2)) OVER (PARTITION BY driverid ORDER BY dataareaid desc,trans_date desc) as \"revenue km%\",\nROUND(\"logged off km%\",2)-lead(ROUND(\"logged off km%\",2)) OVER (PARTITION BY driverid ORDER BY dataareaid desc,trans_date desc) as \"logged off km%\",\nROUND(\"unauthorized leaves\",2)-lead(ROUND(\"unauthorized leaves\",2)) OVER (PARTITION BY driverid ORDER BY dataareaid desc,trans_date desc) as \"unauthorized leaves\",\nROUND(\"itc acceptance rate\",2)-lead(ROUND(\"itc acceptance rate\",2)) OVER (PARTITION BY driverid ORDER BY dataareaid desc,trans_date desc) as \"itc acceptance rate\",\nROUND(outstanding,2)-lead(ROUND(outstanding,2)) OVER (PARTITION BY driverid ORDER BY dataareaid desc,trans_date desc) as outstanding,\nROUND(\"missed deposits\",2)-lead(ROUND(\"missed deposits\",2)) OVER (PARTITION BY driverid ORDER BY dataareaid desc,trans_date desc) as \"missed deposits\",\nROUND(\"hala acceptance rate\",2)-lead(ROUND(\"hala acceptance rate\",2)) OVER (PARTITION BY driverid ORDER BY dataareaid desc,trans_date desc) as \"hala acceptance rate\",\nROUND(\"hala online rate\",2)-lead(ROUND(\"hala online rate\",2)) OVER (PARTITION BY driverid ORDER BY dataareaid desc,trans_date desc) as \"hala online rate\",\nROUND(\"customer feedback\",2)-lead(ROUND(\"customer feedback\",2)) OVER (PARTITION BY driverid ORDER BY dataareaid desc,trans_date desc) as \"customer feedback\",\nROUND(\"personal hygiene\",2)-lead(ROUND(\"personal hygiene\",2)) OVER (PARTITION BY driverid ORDER BY dataareaid desc,trans_date desc) as \"personal hygiene\",\nROUND(\"car cleanliness\",2)-lead(ROUND(\"car cleanliness\",2)) OVER (PARTITION BY driverid ORDER BY dataareaid desc,trans_date desc) as \"car cleanliness\",\nROUND(\"behaviour with customer\",2)-lead(ROUND(\"behaviour with customer\",2)) OVER (PARTITION BY driverid ORDER BY dataareaid desc,trans_date desc) as \"behaviour with customer\", \"Red Signal Crossing\"\nFROM agi_carstaxi_gold_layer.ct_driver_scorecard_all_emirates_new\nwhere driverid = ? and dataareaid= ?\norder by trans_date desc) where rating_Year = ? and rating_month = ?"
